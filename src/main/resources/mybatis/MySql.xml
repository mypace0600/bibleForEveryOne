<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MySql">

    <insert id="insertBible" parameterType="com.best.bible.data.BibleData">
        INSERT INTO TB_BIBLE_TEXT (bookNum,book,chapter,verse,text)
        VALUES (#{bookNum},#{book},#{chapter},#{verse},#{text});
    </insert>


</mapper>
